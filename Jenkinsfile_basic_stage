//https://jenkins.io/doc/book/pipeline/getting-started/#_footnotedef_1
pipeline {
    //select the executor
    agent any
    //define the stages
    stages {
        //define the stage and provide the name in ()
        stage ('Example') {
            //define the steps
            steps {
                //commands
                echo "Hello Class AWT02"
            }
        }
    }
}
